{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "import/extensions": [0, { "js": "always" }],
    "linebreak-style": 0,
    "no-prototype-builtins": 0,
    "no-param-reassign": 0,
    "spaced-comment": 0,
    "eslint max-len": 0,
    "eslint no-else-return": true
  }
}
